    IEEE_80211
    block_device
    cdimage
    cpuinfo
    display
    dmi
    dpkg
    efi
    environment
    gconf
    lsb
    meminfo
    mobilebroadband
    module
    optical_drive_.*                                       certification-status=blocker
    package
    rtc
    sleep
    uname
    usb
    xinput
