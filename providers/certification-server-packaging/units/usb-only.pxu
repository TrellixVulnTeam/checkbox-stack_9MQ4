id: usb-only
_name: USB Only Test Plan (Only tests USB ports)
unit: test plan
_description:
    This test plan is intended to be used for retesting of USB devices
    only.  It does not provide any testing of other hardware and should only be
    run at the direction of the Cert Team when re-testing of USB devices is
    requried.
mandatory_include:
    # Meta-job to include required resources, please don't remove
    miscellanea/submission-resources
    config_file_attachment
    cpuinfo_attachment
    dkms_info_attachment
    dmesg_attachment
    dmi_attachment
    dmidecode_attachment
    efi_attachment
    lspci_attachment
    lshw_attachment
    lsusb_attachment
    lsblk_attachment
    meminfo_attachment
    modprobe_attachment
    modinfo_attachment
    modules_attachment
    sysctl_attachment
    sysfs_attachment
    udev_attachment
    udev-summary_attachment
    lsmod_attachment
    miscellanea/get_make_and_model
    miscellanea/get_maas_version               certification-status=blocker
    miscellanea/check_prerelease               certification-status=non-blocker
    miscellanea/boot_mode
    info/disk_partitions
    kernel_cmdline_attachment
include:
    cpu/purley_id                              certification-status=non-blocker
    info/hdparm_.*.txt
    usb/detect
    usb/storage-preinserted                    certification-status=blocker
    usb3/storage-preinserted                   certification-status=blocker
    miscellanea/oops                           certification-status=non-blocker
    miscellanea/oops_results.log               certification-status=non-blocker
bootstrap_include:
    device
    fwts
